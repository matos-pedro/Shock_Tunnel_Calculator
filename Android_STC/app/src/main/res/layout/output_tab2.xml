                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="1dp"
                android:text="* Insert Pitot Pressure"
                android:textIsSelectable="true"
                android:textSize="19sp"
                android:textStyle="italic"
